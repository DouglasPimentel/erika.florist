---
import WikiLayout from "$layouts/WikiLayout.astro"
import { postProcessWikiItem, WikiItem } from "$data/wiki"

let wikiIndex = await Astro.glob<WikiItem>("/content/wiki/index.md")
wikiIndex[0].frontmatter = postProcessWikiItem(wikiIndex[0].frontmatter, wikiIndex[0].file)
---

<WikiLayout wikiItem={wikiIndex[0]} />
