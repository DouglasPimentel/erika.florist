{
  "name": "princesseuh.dev",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "astro dev --experimental-integrations",
    "build": "astro build --experimental-integrations && npm run copy:images",
    "preview": "astro preview --experimental-integrations",
    "format": "prettier -w --plugin-search-dir=. --cache .",
    "lint": "eslint . --ext .ts,.js,.astro",
    "copy:images": "cp -r static/assets/images dist/assets/"
  },
  "devDependencies": {
    "@astrojs/tailwind": "^0.2.2",
    "astro": "1.0.0-beta.63",
    "astro-auto-import": "^0.1.2",
    "astro-eleventy-img": "^0.4.0",
    "astro-icon": "0.7.1",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-astro": "^0.14.0",
    "eslint-plugin-prettier": "^4.2.1",
    "netlify-plugin-cache": "^1.0.3",
    "prettier": "^2.7.1",
    "prettier-plugin-astro": "^0.1.1",
    "quick-score": "^0.2.0",
    "quicklink": "^2.2.0"
  }
}
