{
  "name": "erika.florist",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "build": "astro build && npm run copy:images",
    "preview": "astro preview",
    "check": "astro check",
    "lint": "eslint . --ext .ts,.js,.astro",
    "format": "prettier -w --plugin-search-dir=. --cache .",
    "copy:images": "cp -r static/assets/images dist/assets/"
  },
  "devDependencies": {
    "@astrojs/tailwind": "^2.1.2",
    "@types/eslint": "^8.4.10",
    "@types/node": "^18.11.9",
    "@types/prettier": "^2.7.1",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "astro": "^1.6.8",
    "astro-auto-import": "^0.2.0",
    "astro-eleventy-img": "^0.5.0",
    "astro-icon": "0.8.0",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-astro": "^0.21.0",
    "eslint-plugin-prettier": "^4.2.1",
    "netlify-plugin-cache": "^1.0.3",
    "prettier": "^2.7.1",
    "prettier-plugin-astro": "^0.7.0",
    "quick-score": "^0.2.0",
    "quicklink": "^2.3.0",
    "typescript": "^4.9.3"
  },
  "pnpm": {
    "packageExtensions": {
      "quicklink": {
        "peerDependenciesMeta": {
          "react": {
            "optional": true
          },
          "react-dom": {
            "optional": true
          }
        }
      }
    }
  },
  "dependencies": {
    "@astrojs/mdx": "^0.11.6",
    "tailwindcss": "^3.2.4"
  }
}
