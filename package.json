{
  "name": "erika.florist",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "build": "astro build && npm run copy:images",
    "preview": "astro preview",
    "check": "astro check",
    "lint": "eslint . --ext .ts,.js,.astro",
    "format": "prettier -w --plugin-search-dir=. --cache .",
    "copy:images": "cp -r static/assets/images dist/assets/"
  },
  "devDependencies": {
    "@astrojs/tailwind": "^2.0.1",
    "@types/eslint": "^8.4.6",
    "@types/node": "^18.7.18",
    "@types/prettier": "^2.7.0",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "astro": "^1.3.0",
    "astro-auto-import": "^0.1.2",
    "astro-eleventy-img": "^0.5.0",
    "astro-icon": "0.7.3",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-astro": "^0.19.0",
    "eslint-plugin-prettier": "^4.2.1",
    "netlify-plugin-cache": "^1.0.3",
    "prettier": "^2.7.1",
    "prettier-plugin-astro": "^0.5.4",
    "quick-score": "^0.2.0",
    "quicklink": "^2.3.0",
    "typescript": "^4.8.3"
  },
  "pnpm": {
    "packageExtensions": {
      "quicklink": {
        "peerDependenciesMeta": {
          "react": {
            "optional": true
          },
          "react-dom": {
            "optional": true
          }
        }
      }
    }
  },
  "dependencies": {
    "tailwindcss": "^3.1.8"
  }
}
