{
  "name": "erika.florist",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "build": "astro build && npm run copy:images",
    "preview": "astro preview",
    "check": "astro check",
    "lint": "eslint . --ext .ts,.js,.astro",
    "format": "prettier -w --plugin-search-dir=. --cache .",
    "copy:images": "cp -r static/assets/images dist/assets/"
  },
  "devDependencies": {
    "@astrojs/tailwind": "^3.0.1",
    "@types/eslint": "^8.21.1",
    "@types/node": "^18.14.0",
    "@types/prettier": "^2.7.2",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "astro": "^2.0.14",
    "astro-auto-import": "^0.2.1",
    "astro-eleventy-img": "^0.6.2",
    "astro-icon": "0.8.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-astro": "^0.23.0",
    "eslint-plugin-prettier": "^4.2.1",
    "netlify-plugin-cache": "^1.0.3",
    "prettier": "^2.8.4",
    "prettier-plugin-astro": "^0.8.0",
    "quick-score": "^0.2.0",
    "quicklink": "^2.3.0",
    "typescript": "^4.9.5"
  },
  "pnpm": {
    "packageExtensions": {
      "quicklink": {
        "peerDependenciesMeta": {
          "react": {
            "optional": true
          },
          "react-dom": {
            "optional": true
          }
        }
      }
    }
  },
  "dependencies": {
    "@astrojs/mdx": "^0.17.0",
    "prettier-plugin-tailwindcss": "^0.2.3",
    "tailwindcss": "^3.2.7"
  }
}
